<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>LogInPageUsingRequestDispatcher</display-name>
  <welcome-file-list>
    <welcome-file>LoginNew.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>registration</servlet-name>
  <servlet-class>com.logInPageUsingRD.NewRegistration</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>registration</servlet-name>
  <url-pattern>/reg</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>com.logInPageUsingRD.LogInVerification</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/log</url-pattern>
  </servlet-mapping>
</web-app>